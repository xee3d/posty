# .env 파일 - 실제 값은 본인의 API 키로 교체하세요

# AI API Keys
OPENAI_API_KEY=your_openai_api_key_here
CLAUDE_API_KEY=your_claude_api_key_here

# Social Login Keys
# Google
GOOGLE_WEB_CLIENT_ID=your_google_web_client_id_here

# Naver
NAVER_CONSUMER_KEY=your_naver_consumer_key_here
NAVER_CONSUMER_SECRET=your_naver_consumer_secret_here
NAVER_APP_NAME=Posty
NAVER_SERVICE_URL_SCHEME=postynaverlogin

# Kakao
KAKAO_APP_KEY=your_kakao_app_key_here
KAKAO_APP_SCHEME=kakaoyour_kakao_app_key_here

# Social Media API Keys
INSTAGRAM_CLIENT_ID=your_instagram_client_id
INSTAGRAM_CLIENT_SECRET=your_instagram_client_secret
FACEBOOK_APP_ID=your_facebook_app_id
FACEBOOK_APP_SECRET=your_facebook_app_secret
TWITTER_API_KEY=your_twitter_api_key
TWITTER_API_SECRET=your_twitter_api_secret

# Backend Server
API_BASE_URL=http://localhost:3000/api
WEBSOCKET_URL=ws://localhost:3000

# Analytics
GOOGLE_ANALYTICS_ID=your_ga_id
MIXPANEL_TOKEN=your_mixpanel_token

# Push Notification
FCM_SERVER_KEY=your_fcm_server_key
APNS_KEY_ID=your_apns_key_id

# Other Services
SENTRY_DSN=your_sentry_dsn
CLOUDINARY_URL=your_cloudinary_url